<body>
    <header>
        <!-- Navbar-->
        <% include partials/navbar %>
    <!-- Main container-->
    <div class="container">
        <!--Page heading-->
        <div class="row wow fadeIn pt-5" data-wow-delay="0.2s">
            <div class="col-md-12 pt-5">
                <h1 class="h1-responsive">Blog |
                    <small class="text-muted">Changelogs</small>
                </h1>
            </div>
        </div>
        <!--/.Page heading-->
        <div class="divider-new pt-5">
            <h2 class="h2-responsive wow fadeIn" data-wow-delay="0.2s">Version 2.0 Released!</h2>
        </div>
            <!-- Section: Status Cards-->
        <section class="text-center wow fadeIn" id="about" data-wow-delay="0.2s">
                <div class="col-md-12 mb-3">

                        <img src="https://cdn.discordapp.com/attachments/371762996206370817/403267925093384197/2.0_release_banner.jpg" class="img-fluid z-depth-1" alt="Responsive image">
                
                    </div>
                    <script src="https://gist.github.com/MattheousDT/771d7aaa5c4a205d87b39bca1185633d.js"></script>
                <a href="changelogs"><button class="btn btn-indigo">View Change Notes</button></a>
        </section>
        <!-- Section: About-->
    </div>
    <!-- / Main container-->
    <!-- Footer-->
    <% include partials/footer %>
        <!-- /.Footer-->
        <!-- SCRIPTS-->
        <!-- JQuery-->
        <script type="text/javascript" src="js/jquery-3.2.1.min.js"></script>
        <!-- Bootstrap dropdown-->
        <script type="text/javascript" src="js/popper.min.js"></script>
        <!-- Bootstrap core JavaScript-->
        <script type="text/javascript" src="js/bootstrap.min.js"></script>
        <!-- MDB core JavaScript-->
        <script type="text/javascript" src="js/mdb.min.js"></script>
        <!-- Animations init-->
        <script>
            new WOW().init();
        </script>

        <script>
            const keyMap = {
                37: "left",
                38: "up",
                39: "right",
                40: "down",
                65: "a",
                66: "b"
            };

            const konamiCode = ["up", "up", "down", "down", "left", "right", "left", "right", "b", "a"];

            var konamiPosition = 0;

            document.addEventListener("keydown", function (e) {
                console.log(konamiPosition);
                if (keyMap[e.keyCode] == konamiCode[konamiPosition]) {

                    konamiPosition++;

                    if (konamiPosition == konamiCode.length) {
                        success();
                        konamiPosition = 0;
                    }
                } else {
                    konamiPosition = 0;
                }

            });

            function success() {

                document.styleSheets[8].cssRules[0].selectorText = "*";
            }
        </script>

</body>

</html>